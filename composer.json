{
    "name": "xsites/laravel-socket.io",
    "license": "MIT",
    "description": "Laravel Socket.io broadcaster",
    "keywords": ["laravel", "laravel-socket.io", "socket.io"],
    "authors": [
        {
            "name": "Anton Pavlov",
            "email": "anton@xsites.co.il"
        }
    ],
    "minimum-stability": "stable",
    "support": {
        "email": "admin@xsites.co.il",
        "issues": "https://github.com/XSites/laravel-socket.io/issues"
    },
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.*",
        "illuminate/broadcasting": "^5.2",
        "exls/socket.io-emitter": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },
    "suggest": {
        "illuminate/support": "Required for Laravel support"
    },
    "autoload": {
        "psr-4": {
            "Xsites\\LaravelSocketIO\\": "src/"
        }
    }
}
